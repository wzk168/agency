package com.agency.koda.model;

import java.io.Serializable;

/**
 *保险产品险种大类
 *@author meng
 *2015-08-05 
 */

@SuppressWarnings("serial")
public class ProductClass implements Serializable
{
	/**
	 *产品ID
	 *pcg_prodclass_id 
	 */
	private String pcgProdclassId;

	/**
	 * 产品名称
	 * pcg_prodclass_name
	 */
	private String pcgProdclassName;

	public String getPcgProdclassId() {
		return pcgProdclassId;
	}

	public void setPcgProdclassId(String pcgProdclassId) {
		this.pcgProdclassId = pcgProdclassId;
	}

	public String getPcgProdclassName() {
		return pcgProdclassName;
	}

	public void setPcgProdclassName(String pcgProdclassName) {
		this.pcgProdclassName = pcgProdclassName;
	}
}
