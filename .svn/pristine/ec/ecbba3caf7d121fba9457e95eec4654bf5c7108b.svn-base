package com.agency.koda.model;

import java.io.Serializable;

/**
 *部门对象
 *@author meng 
 */

@SuppressWarnings("serial")
public class DeptInfo implements Serializable
{

	/**
	 * 部门ID
	 * ds_dept_id
	 */
	private String dsDeptId;

	/**
	 * 部门名称
	 * ds_dept_name
	 */
	private String dsDeptName;

	/**
	 * 部门简称
	 * ds_dept_abbreviation
	 */
	private String dsDeptAbbreviation;

	/**
	 * 部门负责人
	 * ds_dept_principal 
	 */
	private String dsDeptPrincipal;

	/**
	 * 部门类型
	 * ds_dept_type
	 */
	private int ds_dept_type;

	/**
	 * 部门ID
	 * ds_dept_address
	 */
	private String dsDeptAddress;

	/**
	 * 部门电话1
	 * ds_dept_onephone
	 */
	private String dsDeptOnephone;

	/**
	 * 部门电话2
	 * ds_dept_twophone
	 */
	private String dsDeptTwophone;

	/**
	 * 部门邮件
	 * ds_dept_email
	 */
	private String dsDeptEmail;
	
	/**
	 * 使用公司
	 * ds_company_id
	 */
	private String dsCompanyId;

	public String getDsDeptId() {
		return dsDeptId;
	}

	public void setDsDeptId(String dsDeptId) {
		this.dsDeptId = dsDeptId;
	}

	public String getDsDeptName() {
		return dsDeptName;
	}

	public void setDsDeptName(String dsDeptName) {
		this.dsDeptName = dsDeptName;
	}

	public String getDsDeptAbbreviation() {
		return dsDeptAbbreviation;
	}

	public void setDsDeptAbbreviation(String dsDeptAbbreviation) {
		this.dsDeptAbbreviation = dsDeptAbbreviation;
	}

	public String getDsDeptPrincipal() {
		return dsDeptPrincipal;
	}

	public void setDsDeptPrincipal(String dsDeptPrincipal) {
		this.dsDeptPrincipal = dsDeptPrincipal;
	}

	public int getDs_dept_type() {
		return ds_dept_type;
	}

	public void setDs_dept_type(int ds_dept_type) {
		this.ds_dept_type = ds_dept_type;
	}

	public String getDsDeptAddress() {
		return dsDeptAddress;
	}

	public void setDsDeptAddress(String dsDeptAddress) {
		this.dsDeptAddress = dsDeptAddress;
	}

	public String getDsDeptOnephone() {
		return dsDeptOnephone;
	}

	public void setDsDeptOnephone(String dsDeptOnephone) {
		this.dsDeptOnephone = dsDeptOnephone;
	}

	public String getDsDeptTwophone() {
		return dsDeptTwophone;
	}

	public void setDsDeptTwophone(String dsDeptTwophone) {
		this.dsDeptTwophone = dsDeptTwophone;
	}

	public String getDsDeptEmail() {
		return dsDeptEmail;
	}

	public void setDsDeptEmail(String dsDeptEmail) {
		this.dsDeptEmail = dsDeptEmail;
	}

	public String getDsCompanyId() {
		return dsCompanyId;
	}

	public void setDsCompanyId(String dsCompanyId) {
		this.dsCompanyId = dsCompanyId;
	}

	
}
