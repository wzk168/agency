package com.agency.koda.model;

import java.io.Serializable;

/**
 * 角色对象
 * 
 * @author Waco
 *
 */
public class RoleInfo implements Serializable {
	private static final long serialVersionUID = 1L;
	/**
	 * 角色ID
	 */
	private String rosRoleinfoId;
	/**
	 * 角色名称
	 */
	private String rosRoleinfoName;
	/**
	 * 角色备注
	 */
	private String rosRoleinfoRemark;
	/**
	 * 公司ID
	 */
	private String rosCompanyId;

	public String getRosRoleinfoId() {
		return rosRoleinfoId;
	}

	public void setRosRoleinfoId(String rosRoleinfoId) {
		this.rosRoleinfoId = rosRoleinfoId;
	}

	public String getRosRoleinfoName() {
		return rosRoleinfoName;
	}

	public void setRosRoleinfoName(String rosRoleinfoName) {
		this.rosRoleinfoName = rosRoleinfoName;
	}

	public String getRosRoleinfoRemark() {
		return rosRoleinfoRemark;
	}

	public void setRosRoleinfoRemark(String rosRoleinfoRemark) {
		this.rosRoleinfoRemark = rosRoleinfoRemark;
	}

	public String getRosCompanyId() {
		return rosCompanyId;
	}

	public void setRosCompanyId(String rosCompanyId) {
		this.rosCompanyId = rosCompanyId;
	}

	public RoleInfo() {
		super();
	}

	public RoleInfo(String rosRoleinfoId, String rosRoleinfoName,
			String rosRoleinfoRemark, String rosCompanyId) {
		super();
		this.rosRoleinfoId = rosRoleinfoId;
		this.rosRoleinfoName = rosRoleinfoName;
		this.rosRoleinfoRemark = rosRoleinfoRemark;
		this.rosCompanyId = rosCompanyId;
	}

}
